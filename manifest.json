{
  "name": "Chrome Extension MV3",
  "description": "Learning all about Chrome Extensions Manifest Version 3!",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/logo.png",
    "32": "/logo.png",
    "48": "/logo.png",
    "128": "/logo.png"
  },
  "action": {
    "default_popup": "/popup.html",
    "default_icon": {
      "16": "/logo.png",
      "32": "/logo.png",
      "48": "/logo.png",
      "128": "/logo.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"]
}
